{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "semicolon": [
            true,
            "never"
        ],
        "trailing-comma": [
            true,
            "never"
        ]
    },
    "rulesDirectory": []
}